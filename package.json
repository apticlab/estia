{
    "name": "@apticlab/estia",
    "version": "1.7.0",
    "description": "The easiest way to build site in Vue.",
    "main": "dist/estia.umd.js",
    "module": "dist/estia.esm.js",
    "unpkg": "dist/estia.min.js",
    "publishConfig": {
        "access": "public"
    },
    "browser": {
        "./sfc": "src/estia.js"
    },
    "scripts": {
        "watch": "rollup --config build/rollup.config.js --format es --file dist/estia.esm.js -w",
        "build:dev": "rollup --config build/rollup-dev.config.js --format es --file dist/estia.esm.js -w",
        "build": "npm run build:esm & npm run build:umd && wait && echo \"Build complete:\nesm: $(gzip -c dist/estia.esm.js | wc -c)b gzip\numd: $(gzip -c dist/estia.umd.js | wc -c)b gzip\nmin: $(gzip -c dist/estia.min.js | wc -c)b gzip\"",
        "build:esm": "rollup --config build/rollup.config.js --format es --file dist/estia.esm.js",
        "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/estia.umd.js",
        "build:iife": "rollup --config build/rollup.iife.config.js --format iife --file dist/estia.min.js",
        "build:css": "node-sass themes/snow/snow.scss dist/snow.css && postcss --use autoprefixer -b '> 2%' < dist/snow.css | postcss --no-map --use cssnano > dist/snow.min.css",
        "test": "NODE_ENV=test jest --config test/jest.conf.js --coverage",
        "test:watch": "NODE_ENV=test jest --config test/jest.conf.js --watch",
        "test:coverage": "NODE_ENV=test jest --config test/jest.conf.js --coverage",
        "build:size": "gzip -c dist/estia.esm.js | wc -c",
        "preversion": "npm run build && git add .",
        "postversion": "npm publish",
        "dev": "vue-cli-service serve --port=7872 examples/simple-app/src/main.js",
        "dev:build": "vue-cli-service serve src/main.js"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/apticlab/estia.git"
    },
    "keywords": [
        "vue",
        "form",
        "forms",
        "validation",
        "vuex",
        "validate",
        "table",
        "api"
    ],
    "author": "Isacco Rossi <ir.isacco.rossi@gmail.com>; Giacomo Alberini <giacomoalbe@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/apticlab/estia/issues"
    },
    "homepage": "https://github.com/apticlab/estia",
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/plugin-transform-modules-commonjs": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@rollup/plugin-alias": "^3.1.9",
        "@rollup/plugin-buble": "^0.21.3",
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@vue/cli-plugin-babel": "^4.5.15",
        "@vue/cli-plugin-eslint": "^4.5.15",
        "@vue/cli-service": "^4.5.15",
        "@vue/component-compiler-utils": "^3.3.0",
        "@vue/test-utils": "^1.0.2",
        "acorn": "^8.5.0",
        "autoprefixer": "^9",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^25.5.1",
        "cssnano": "^4.1.10",
        "eslint": "^8.8.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-standard": "^5.0.0",
        "eslint-plugin-vue": "^8.4.1",
        "flush-promises": "^1.0.2",
        "jest": "^25.5.4",
        "jest-vue-preprocessor": "^1.7.1",
        "node-sass": "^4.14.1",
        "postcss": "^8.4.6",
        "resolve": "^1.20.0",
        "rollup": "^2.67.0",
        "rollup-plugin-auto-external": "^2.0.0",
        "rollup-plugin-internal": "^1.0.4",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-multi-input": "^1.3.1",
        "rollup-plugin-scss": "^3.0.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-vue": "^5.1.7",
        "sass-loader": "^8.0.2",
        "vue": "^2.6.14",
        "vue-jest": "^3.0.7",
        "vue-runtime-helpers": "^1.1.2",
        "vue-svgicon": "^3.2.9",
        "vue-template-compiler": "^2.6.14",
        "vue-template-es2015-compiler": "^1.9.1",
        "watch": "^1.0.2"
    },
    "dependencies": {
        "@braid/vue-formulate": "^2.5.3",
        "@popperjs/core": "^2.11.2",
        "axios": "^0.25.0",
        "chart.js": "^2.9.3",
        "codice-fiscale-js": "^2.3.14",
        "colorsys": "^1.0.22",
        "core-js": "^3.21.0",
        "d3": "^7.3.0",
        "d3-queue": "^3.0.7",
        "eslint-utils": "^3.0.0",
        "is-plain-object": "^5.0.0",
        "is-url": "1.2.4",
        "jspdf": "^2.3.1",
        "jsvat": "^2.5.3",
        "lodash": "4.17.21",
        "moment": "^2.29.1",
        "nanoid": "^3.1.23",
        "popper.js": "^1.16.1",
        "prismjs": "^1.21.0",
        "survey-analytics": "^1.7.26",
        "survey-creator": "^1.7.26",
        "survey-pdf": "^1.7.26",
        "survey-vue": "^1.7.26",
        "surveyjs-widgets": "^1.7.26",
        "tailwindcss": "3.0.18",
        "v-calendar": "^2.3.1",
        "v-lazy-image": "^1.4.0",
        "vue": "^2.6.14",
        "vue-i18n": "^8.24.5",
        "vue-popper-component": "^0.1.0",
        "vue-prism-editor": "^0.6.1",
        "vue-router": "^3.5.2",
        "vue-select": "^3.11.2",
        "vue-tailwind": "^2.5.0",
        "vue-window-size": "^0.2.5",
        "vue2-daterange-picker": "^0.6.5",
        "vue2-timepicker": "^1.1.3",
        "vuejs-datepicker": "^1.6.2",
        "vuex": "^3.6.2",
        "xlsx": "^0.17.0"
    }
}
