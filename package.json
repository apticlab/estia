{
  "name": "@apticlab/estia",
  "version": "1.1.4",
  "description": "The easiest way to build site in Vue.",
  "main": "dist/estia.umd.js",
  "module": "dist/estia.esm.js",
  "unpkg": "dist/estia.min.js",
  "publishConfig": {
    "access": "public"
  },
  "browser": {
    "./sfc": "src/estia.js"
  },
  "scripts": {
    "build": "npm run build:esm & npm run build:umd & npm run build:iife & wait && echo \"Build complete:\nesm: $(gzip -c dist/estia.esm.js | wc -c)b gzip\numd: $(gzip -c dist/estia.umd.js | wc -c)b gzip\nmin: $(gzip -c dist/estia.min.js | wc -c)b gzip\"",
    "build:esm": "rollup --config build/rollup.config.js --format es --file dist/estia.esm.js",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/estia.umd.js",
    "build:iife": "rollup --config build/rollup.iife.config.js --format iife --file dist/estia.min.js",
    "build:css": "node-sass themes/snow/snow.scss dist/snow.css && postcss --use autoprefixer -b '> 2%' < dist/snow.css | postcss --no-map --use cssnano > dist/snow.min.css",
    "test": "NODE_ENV=test jest --config test/jest.conf.js --coverage",
    "test:watch": "NODE_ENV=test jest --config test/jest.conf.js --watch",
    "test:coverage": "NODE_ENV=test jest --config test/jest.conf.js --coverage",
    "build:size": "gzip -c dist/estia.esm.js | wc -c",
    "dev": "vue-cli-service serve --port=7872 examples/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/apticlab/estia.git"
  },
  "keywords": [
    "vue",
    "form",
    "forms",
    "validation",
    "vuex",
    "validate",
    "table",
    "api"
  ],
  "author": "Isacco Rossi <ir.isacco.rossi@gmail.com>; Giacomo Alberini <giacomoalbe@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apticlab/estia/issues"
  },
  "homepage": "https://github.com/apticlab/estia",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/component-compiler-utils": "^3.1.2",
    "@vue/test-utils": "^1.0.2",
    "autoprefixer": "^9.7.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "flush-promises": "^1.0.2",
    "jest": "^25.5.4",
    "jest-vue-preprocessor": "^1.7.1",
    "node-sass": "^4.14.1",
    "postcss": "^7.0.30",
    "postcss-cli": "^7.1.1",
    "rollup": "^1.32.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-internal": "^1.0.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.7",
    "sass-loader": "^8.0.2",
    "typescript": "^3.9.2",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-runtime-helpers": "^1.1.2",
    "vue-template-compiler": "^2.6.11",
    "vue-template-es2015-compiler": "^1.9.1",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "is-plain-object": "^3.0.0",
    "is-url": "^1.2.4",
    "nanoid": "^2.1.11",
    "@braid/vue-formulate": "^2.4.3",
    "@popperjs/core": "^2.4.4",
    "axios": "^0.19.2",
    "chart.js": "^2.9.3",
    "colorsys": "^1.0.22",
    "core-js": "^3.4.4",
    "d3": "^5.15.0",
    "d3-queue": "^3.0.7",
    "jspdf": "^1.5.3",
    "lodash": "4.17.19",
    "moment": "^2.27.0",
    "popper.js": "^1.16.1",
    "prismjs": "^1.21.0",
    "survey-analytics": "^1.7.26",
    "survey-creator": "^1.7.26",
    "survey-pdf": "^1.7.26",
    "survey-vue": "^1.7.26",
    "surveyjs-widgets": "^1.7.26",
    "tailwindcss": "^1.6.2",
    "v-calendar": "^1.0.8",
    "v-lazy-image": "^1.4.0",
    "vue": "^2.6.10",
    "vue-country-flag": "^1.3.1",
    "vue-i18n": "^8.20.0",
    "vue-popper-component": "^0.1.0",
    "vue-prism-editor": "^0.6.1",
    "vue-router": "^3.4.3",
    "vue-select": "^3.10.8",
    "vue-window-size": "^0.2.5",
    "vue2-daterange-picker": "^0.4.6",
    "vue2-timepicker": "^1.1.3",
    "vuejs-datepicker": "^1.6.2",
    "vuex": "^3.5.1",
    "xlsx": "^0.15.5"
  }
}
